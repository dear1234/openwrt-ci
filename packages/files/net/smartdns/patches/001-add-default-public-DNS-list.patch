--- a/package/openwrt/files/etc/config/smartdns
+++ b/package/openwrt/files/etc/config/smartdns
@@ -1,3 +1,66 @@
 config 'smartdns'
 	option 'enabled' '0'
+
+config server
+	option type 'udp'
